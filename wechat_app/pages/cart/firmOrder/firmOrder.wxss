page{
  background-color: #f8f8f8;
}
.firmorder-content{
  padding-bottom:100rpx;
}
.userinfo{
  margin-bottom: 16rpx;
  position: relative;
  padding-bottom: 6rpx;
  background-color: #fff;
}
.userinfo-c{
  padding: 30rpx 30rpx 20rpx;
  overflow: hidden; 
  display: flex;
  position: relative;
  min-height: 104rpx;
  box-sizing: border-box;
}
.user-name-phone{
  margin-bottom: 16rpx;
  font-size:28rpx;
  background-color: #fff;
}
.user-name-phone>view{
  height:80rpx;
  line-height:80rpx;
  padding: 0rpx 30rpx;
  overflow: hidden;
}
.user-name-phone>view>text{
  display: inline-block;
  width: 100rpx;
  height:80rpx;
  line-height:80rpx;
  float: left;
  color:#666;
}
.user-name-phone>view>input{
  display: inline-block;
  width: 500rpx;
  height:80rpx;
  line-height:80rpx;
}
.user-n-p-input{
  color: #999;
  font-size: 26rpx;
}
.user-name{  
  border-bottom: 2rpx solid #f3f3f3;
}
.gps-img{
  width: 44rpx;
  height: 44rpx;
  display: inline-block;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.userinfo-body{
  display: inline-block;
  width: 620rpx;
  overflow: hidden;
  font-size: 28rpx;
  margin-left: 76rpx 
}
.userinfo-body>text{
  position: relative;
  margin-left: 10rpx;
  color: #333;
}
.userinfo-body-top{
  margin-bottom: 10rpx;
  overflow: hidden;
}
.consignee,.consignee-name{
  font-size: 28rpx;
  color: #333;
}
.phone-num{
  font-size: 26rpx;
  color: #333;
  float: right;
}
.address{
  display: inline-block;
  width: 572rpx;
}
.address text{
  font-size: 26rpx;
  color: #999;
}
.userinfo-body-bottom{
  overflow: hidden;
  display: flex;
}
.userinfo-body-bottom image{
  width: 14rpx;
  height: 24rpx; 
  float: right;
  margin: auto;
}
.list-details{
  padding: 10rpx 30rpx;
  background-color: #f9f9f9;
  overflow: hidden;
}
.thumbnail{
  width: 160rpx;
  height: 160rpx;
  float: left;
}
.order-list{
  background-color: #fff;
  padding: 20rpx 0 0;
  margin-bottom: 16rpx;
}
.order-details{
  width: 500rpx;
  float: right;
}
.order-details .name{
  width: 500rpx;
  font-size: 26rpx;
  color: #666;
  overflow:hidden;
  text-overflow: ellipsis;  
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  white-space: normal;
}
.size-style{
  font-size: 24rpx;
  color: #999;
}
.price-num{
  width: 500rpx;
  overflow: hidden;
  font-size: 28rpx;
}
.price-num .price{  
  color: #FF3B44;
}
.price-num .num{
  color: #666;
  float: right;
  font-size: 24rpx;
  /* background-color: #fff; */
}
.price-list{
  font-size: 28rpx;
  color: #666;
  background-color: #fff;
  margin-bottom: 16rpx;
}
.price-details{
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx 0 0;
  margin-left: 30rpx;
  border-bottom: 2rpx solid #f1f1f1; 
  overflow: hidden;
}
.price-details:last-child{
  border: none;
}
.price-calculation{
  padding: 20rpx 30rpx 20rpx 0;
  margin-left: 30rpx;
  /* border-bottom: 2rpx solid #f1f1f1;  */
}
.price-calculation>view{
  overflow: hidden;
}
.integral{
    /* margin-top: 6rpx; */
    font-size: 28rpx;
    color: #666;
    overflow: hidden;
}
.integral checkbox{
    float: right;
}
.integral checkbox .wx-checkbox-input{   
    border-radius: 10rpx;
    height: 26rpx;
    width: 26rpx;
    margin-top: -4rpx;
    border: 1rpx solid #888;
}
.integral checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: 1rpx solid #FF525C;
  background: #FF525C;
}
/*checkbox选中后图标样式  */
.integral checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
.integral-details{
  font-size: 24rpx;
  color: #999;
}
.price-calculation>view>.price-details-right{
  color: #FF3B44;
}
.price-details-right{
  float: right;
}
.buyer text{
  float: left;
}
.buyer input{
  height: 80rpx;
  width: 400rpx;
  display: inline-block;
  margin-left: 30rpx;
}
.buyer-input{
  color: #b3b3b3;
}
.firmorder-footer{
  height: 100rpx;
  width: 750rpx;
  line-height: 100rpx;
  /* background-color: #f8f8f8; */
  position: fixed;
  bottom: 0;
  overflow: hidden;
  z-index:999;
  background-color: #fff;
}
.firmorder-footer-right{
  float: right;
  font-size: 32rpx;
  text-align:right;
}
.total-price{
  color: #FF3B44;
}
.immediate-pay{
  width: 212rpx;
  height: 100rpx;
  line-height: 100rpx !important;
  color: #fff !important;
  background-color: #FF3B44 !important;
  float: right;
  display: inline-block;
  text-align: center;
  font-size: 28rpx;
  margin-left: 20rpx;
  border-radius: 0;
}
.gps-ico{
  width:60rpx;
  height:60rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.right-ico{
  width:16rpx;
  height:28rpx;
  float:right;
}
