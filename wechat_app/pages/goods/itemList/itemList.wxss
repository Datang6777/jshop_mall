page{
  background-color: #f3f3f3;
}
.itemlist-content{
  padding-top: 90rpx;
  border-top: 2rpx solid #f4f4f4;
  overflow: hidden;
}
.content-header{
  overflow: hidden;
  width: 700rpx;
  padding: 20rpx 25rpx;
  background-color: #fff;
}
.content-header>view{
  width: 25%;
  text-align: center;
  font-size: 28rpx;
  color: #666;
  float: left;
}
.comprehensive{
  
}
.content-body{
  overflow: hidden;
}
.filter{
  
}
.filter image{
  width: 20rpx;
  height: 20rpx;
}
.sales-volume text,.price text,.sales-volume view,.price view,.comprehensive view{
  display: inline-block;
}
.sales-volume image,.price image,.comprehensive image{
  width: 18rpx;
  height: 10rpx;
  display: block;
  margin-left: 6rpx;
}
.sales-volume image:first-child,.price image:first-child{
  margin-bottom: 4rpx;
}
.comprehensive image{
  margin-bottom: 4rpx;
}
.hide{
 display: none;
}
.show{ 
 display: block;
}
.content-title{
  width: 690rpx;
  height: 84rpx;
  line-height: 84rpx;
  background-color: #f8f8f8;
  color: #d0d0d0;
  padding: 0 30rpx;
  overflow: hidden;
}
.content-title>text{
  font-size: 26rpx;
  color: #666;
}
.content-title-input{
  display: inline-block;
  font-size: 26rpx;
  float: right;
}
.content-title-input-left{
  display: inline-block;
}
.content-title-input-left>input{
  width: 152rpx;
  height: 48rpx;
  background: #fff;
  display: inline-block;
  border-radius: 10rpx;
  margin-top: 18rpx;
  font-size: 26rpx;
  text-align: center;
  margin-right: 20rpx
}
.content-title-input-left>text{
  position: absolute;
  top: 176rpx;  
  left: 388rpx
}
.input-placeholder{
  color: #c8c8c8;
}
.confirm{
  width: 132rpx;
  height: 48rpx;
  display: inline-block;
  font-size: 28rpx;
  color: #fff;
  background-color: #FF3B44;
  text-align: center;
  float: right;
  line-height: 48rpx;
   margin: 20rpx 8rpx 0 0rpx; 
  border-radius: 6rpx;
}
.every-commodity{
  padding: 20rpx 30rpx;  
  margin-bottom: 2rpx;
  overflow: hidden;
  background-color: #fff;
}
.commodity-pic{
  width: 240rpx;
  height: 240rpx;
  border-radius: 6rpx;
}
.every-commodity-right{
  width: 430rpx;
  display: inline-block;
  margin-left: 20rpx;
  /* height: 240rpx; */
  float: right;
  position: relative;
}
.commodity-name{
  font-size: 26rpx;
  color: #666;
  width: 430rpx;
  overflow:hidden;
  text-overflow: ellipsis;  
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  white-space: normal;
}
.preferential{
    /* height: 48rpx; */
    /* overflow: hidden; */
    line-height:36rpx;
}
.preferential>text{
  display: inline-block;
  background-color: #FF3B44;
  color: #fff;
  font-size: 18rpx;
  margin-right: 18rpx;
  border-radius: 10rpx;
  height: 28rpx;
  line-height: 28rpx;
  padding:0 10rpx;
}
.price-cart{
  width: 420rpx;
  overflow: hidden;
  height:52rpx;
}
.price{
  font-size: 32rpx;
  color: #FF3B44;
}
.cart-img{
  width: 52rpx;
  height: 52rpx;
  float: right;
}
.loading{
  text-align: center;
  font-size: 28rpx;
  color: #999;
  margin: 10rpx 0;
  padding-bottom: 10rpx;
}
.lable_green{
  background-color: #009688 !important;
}
.lable_orange{
  background-color: #FFB800!important;
}
.lable_blue{
  background-color: #1E9FFF!important
}
.promotion_off{
  background-color:#ccc !important;
  color:#fff;
  font-size: 18rpx;
  border-radius: 10rpx;
  height:28rpx;
  line-height:28rpx;
  padding: 0 10rpx;
}
.none-img{
  margin: 100rpx auto;
  text-align: center;
}
.none-img image{
  width: 230rpx;
  height: 230rpx;
}
.none-img text{
  display: block;
  color:#ccc;
  font-size:28rpx;
}
.every-commodity-grid{
  width: 345rpx;
  float: left;
  box-sizing: border-box;
  border-bottom: 0;
  /* text-align: center; */
  padding: 0rpx;
  margin: 15rpx;
}
.every-commodity-grid:nth-child(even){
  margin-right: 15rpx;
  margin-left: 0;
}
.every-commodity-grid:nth-child(odd){
  margin-right: 30rpx;
}
.every-commodity-grid>image{
  width: 345rpx;
  height: 345rpx;
}
.every-commodity-grid .every-commodity-right{
  width: 100%;
  height: 236rpx;
  text-align: left;
  box-sizing: border-box;
  padding: 0rpx 20rpx;
}
.every-commodity-grid .commodity-name{
  width: 100%;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  min-height:76rpx;
}
.every-commodity-grid .price-cart{
  width: 100%;
}
.every-commodity-grid .commodity-pic{

}
.search-key{
    position:absolute;
    top:50%;
    transform:translateY(-50%);
    left:76rpx;
}
.placeholder{
    color: #999;
}