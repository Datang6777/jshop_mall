page{
  background-color: #f3f3f3;
}
.nav{
  width:100%;
  height:70rpx;
  line-height: 70rpx;
  display:flex;
  flex-direction:row;
  border-top: 2rpx solid #e8e8e8;
}
.default{
  text-align:center;
  flex:1;
  color:#666;
  font-size:32rpx;
  border-bottom: 2rpx solid #e8e8e8;
}
.red{
  text-align:center;
  color:#FF3B44 ;
  flex:1;
  font-size:32rpx;
  border-bottom: 2rpx solid #FF3B44;
}
.show{
  display:block;
}
.hidden{
  display:none;
}
.listdetails{
  margin-bottom: 16rpx;
  background-color: #fff;
}
.top{
  width: 690rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 30rpx;
  overflow: hidden;
}
.top text{
  font-size: 24rpx;
  color: #FF3B44;
  float: right;
}
.mid-top{
  background-color: #FBFBFB;
  padding: 20rpx 30rpx 8rpx;
  overflow: hidden;
}
.goods-img{
  width: 160rpx;
  height: 160rpx;
}
.mid-top-right{
  display: inline-block;
  width: 500rpx;
  float: right;
}
.name-price{
  overflow: hidden;
}
.name{
  width: 500rpx;
  font-size: 26rpx;
  color: #666;
  overflow:hidden;
  text-overflow: ellipsis;  
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  white-space: normal;
}
.price{
  display: inline-block;
   float: right; 
   margin-top: 6rpx;
}
.cost-price{
  font-size: 24rpx;
  color: #FF3B44;
  margin-right: 10rpx;
}
.current-price{
  font-size: 24rpx;
  color: #B5B5B5;
  margin-top: 6rpx;
}
.style-num{
  width: 500rpx;
  font-size: 24rpx;
  color: #999;
  overflow: hidden;
  margin-top: 14rpx;
}
.num{
  float: right;
  font-size:24rpx;
  color:#999;
}
.mid-bot{
  height: 60rpx;
  line-height: 60rpx;
  width: 690rpx;
  font-size: 24rpx;
  color: #666;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #e8e8e8;
  overflow: hidden;
}
.mid-bot>view{
  float: right;
}
.mid-bot text{
   margin-left: 14rpx;
}
.bottom{
  width:690rpx;
  padding:10rpx 30rpx 14rpx;
  overflow:hidden;
  background-color:#fff;
}
.bottom>view{      
  float: right;    
}
.bottom>view>view{
  text-align:center;
  font-size:24rpx;
  padding:6rpx 20rpx;
  display:inline-block;
  margin-left:26rpx;
}
.bottom-left{
  color: #999;
  border: 2rpx solid #ccc;
}
.bottom-right{
  color:#fff;
  border:2rpx solid #FF3B44;
  background-color:#FF3B44;
}
.loading{
  text-align:center;
  font-size:28rpx;
  margin:20rpx 0;
  color:#bbb;
}
.none-img{
  margin: 100rpx auto;
  text-align: center;
}
.none-img image{
  width: 200rpx;
  height: 200rpx;
}
.none-img text{
  display: block;
  color:#bbb;
  font-size:32rpx;
}
.number{
  float: left !important;
  color: #666 !important;
}
.price-gray{
  color:#696969;
  font-size:22rpx;
  text-decoration:line-through;
}
.market-price{
  height: 32rpx;
  line-height: 32rpx;
  margin-top: 10rpx;
}