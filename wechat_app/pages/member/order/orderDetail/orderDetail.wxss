page{
  background-color:#F9F9F9;
}
.body{
  height: 100%;
}
.delivery{
  border-top: 2rpx solid #d2d2d2;
  position: relative;
  padding: 20rpx 30rpx 0;
  min-height: 100rpx;
  box-sizing: border-box;
}
.delivery>image{
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.delivery-content{
  width: 500rpx;
  display: inline-block;
  margin-left: 100rpx;
}
.delivery-content>text{
  display: block;
}
.content-top{
  font-size: 28rpx;
}
.content-mid{
  margin: 10rpx 0 0rpx;
}
.content-mid ,.content-bot{
  font-size: 22rpx;
  color: #999;
}
.delivery-information{
  width: 690rpx;
  padding: 0 30rpx 14rpx;
  color: #379B2D;
  font-size: 26rpx;
  overflow: hidden;
  border-bottom: 2rpx solid #f1f1f1;
}
.car{
  width: 46rpx;
  height: 36rpx;
  margin: auto;
}
.information-content>text{
  display: block;
  font-size: 24rpx;
}
.information-content{
  display: inline-block;
  width: 606rpx;
  margin: 0 0 0 30rpx;
}
.address .information-content{
  width: 614rpx;
}
.information-time{
  font-size: 24rpx;
  color: #999;
}
.right{
  width: 14rpx;
  height: 24rpx;
  margin: auto;
}
.address{
  border-bottom: 16rpx solid #fff;
  position: relative;
  padding: 0 0rpx 20rpx;
}
.address-c{
  width: 690rpx;
  padding: 20rpx 30rpx;
  overflow: hidden;
  
  font-size: 28rpx;
  color: #999;
  display: flex;
}
.gps{
  width: 46rpx;
  height: 46rpx;
  margin:auto;
}
.username{
  font-size: 28rpx;
  color: #666;
}
.address .right{
  margin-bottom: 40rpx;
}
.username{
  display: inline-block;
  width: 50%;
}
.phone{
  width: 49%;
  display: inline-block;
  text-align: right;
  font-size: 26rpx;
}
.goods{
  margin-bottom: 20rpx;
  background-color: #fff;
  color: #666;
  overflow: hidden;
}
.goods image{
  width: 160rpx;
  height: 160rpx;
}
.goods-lists{
  overflow: hidden;
  padding: 20rpx 30rpx;
  background-color: #F9F9F9;
  border-bottom:1px solid #f1f1f1;
}
.goods-content{
  display: inline-block;
  width: 500rpx;
  float: right; 
  font-size: 30rpx;
  color: #666;
}
.goods-content-header{
  overflow: hidden;
}
.goods-header-left{
  width: 500rpx;
  display: inline-block;
  overflow:hidden;
  text-overflow: ellipsis;  
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  white-space: normal;
  font-size: 26rpx;
}
.goods-header-right{
  margin-right: 10rpx;
  color: #FF3B44;
  font-size: 24rpx;
}
.goods-content-footer{
  overflow: hidden;
  height: 28rpx;
  line-height: 28rpx;
}
.goods-foot{
  padding: 0rpx 30rpx 10rpx;
  text-align: right;
}
.goods-foot-l{
  font-size: 22rpx;
  color: #999;
  margin-right: 20rpx;
}
.goods-foot-mid{
  font-size: 26rpx;
}
.goods-foot-r{
  font-size: 26rpx;
  color: #FF3B44;
}
.pay-information{
  background-color: #fff;
}
.pay-information .weui-cell:last-child{
  border: 0;
}
.style{
  color: #999;
  font-size: 20rpx;
  margin: 14rpx 0; 
  display: block;
}
.num{
  float: right;
  margin-right: 10rpx;
  font-size: 20rpx;
}
.goods-btn{
  width: 160rpx;
  height: 56rpx;
  margin-top: 20rpx;
  line-height: 56rpx;
  border: 2rpx solid #ccc;
  text-align: center;
  background-color: #fff;
  border-radius: 60rpx;
  font-size: 28rpx;
  color: #666;
  float: right;
  margin-right: 60rpx; 
}
.weui-cells{
  position: static;
}
.weui-cell{
  color: #666;
  font-size: 28rpx !important;
  border-bottom: 2rpx solid #f1f1f1;
  position: static;
}
.weui-cell__ft{
  color: #666;
  max-width: 500rpx;
}
.leaving-message {
  overflow: hidden;
  padding:20rpx 30rpx;
  color:#666;
  font-size:28rpx !important;
  border-bottom:2rpx solid #f1f1f1;
}
.leaving-message-l{
  float: left;
}
.leaving-message-r{
  float: right;
  color: #666;
  max-width: 500rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-align: right;
}
/* .leaving-message-r-i{
    float: right;
    margin-bottom: 6rpx
} */
.price{
  color: #FF3B44;
}
.remark{
  width: 690rpx;
  margin: 20rpx auto 120rpx;
  font-size: 24rpx;
  color: #999;
}
.footer{
  width: 750rpx;
  height: 80rpx;
  position: fixed;
  bottom: 0rpx; 
  background-color: #fff;
  overflow: hidden;
}
.footer-left{
  height: 100rpx;
  margin-left: 10rpx;
  max-width: 320rpx;
  font-size: 24rpx;
  float: left;
  display: flex;
}
.left-bot{
  color: #999;
  margin: auto;
}
.footer-right{
  line-height: 80rpx;
  display: inline-block;
  text-align: right;
  font-size: 26rpx;
  float: right;
}
.footer-right>view>view{
  display: inline-block;
  padding: 0 20rpx;
  margin: 10rpx;
  box-sizing: border-box;
  line-height: 50rpx;
}
.red-price{
  color: #FF3B44;
}
.gray-circle{
  color: #999;
  border:2rpx solid #ccc;  
}
.footer-right>view>view:last-child{
  margin-left: 10rpx;
}
.footer-right-red{
  background-color: #FF3B44;
  border: 2rpx solid #FF3B44 !important;
  color: #fff !important;
}
.promotion_on{
  background-color:#FF3B44;
  color:#fff;
  font-size: 18rpx;
  border-radius: 10rpx;
  padding: 0 10rpx;
  display: inline-block;
  height:28rpx;
  line-height:28rpx;
  margin-right: 10rpx;
}
.bottom-line{
  width: 100%;
  height: 6rpx;
  position: absolute;
  bottom: 20rpx;
}
.di-top{
  overflow: hidden;
  width: 100%;
}
.di-top text{
  color: #999;
  width: 33%;
  float: left;
  font-size: 22rpx;
}
.di-top text:nth-child(2){
  text-align: center;
}
.di-top text:nth-child(3){
  text-align: right;
}
.green-text{
  color: #06be04 !important; 
}
.di-bottom{
  width: 100%;
}
.di-bottom>image{
  width: 24rpx;
  height: 24rpx;
  display: inline-block;
}
.di-bottom>view{
  border-bottom: 2rpx solid #f1f1f1;
  width: 276rpx;
  display: inline-block;
  margin: 0 16rpx;
  position:relative;
  bottom:10rpx;
}
.price-c {
  padding: 20rpx 0 0;
}
.price-c .weui-cell{
  padding: 0 30rpx;
  border-bottom: 0;
  font-size: 24rpx !important;
}
.dbck{
    width: 100%;
    height: 90rpx;
}
.order-tips{
    font-size: 28rpx;
    color: #333;
    padding: 20rpx 30rpx;
    border-bottom:2rpx solid #f1f1f1;
}
.order-tips view{
    display: inline-block;
    width: 28rpx;
    height: 28rpx;
    border-radius: 50%;
    color: #fff;
    background-color: #FF3B44;
    line-height: 30rpx;
    text-align: center;
    font-size: 20rpx;
}
.deliverynone{
  display: none;
}