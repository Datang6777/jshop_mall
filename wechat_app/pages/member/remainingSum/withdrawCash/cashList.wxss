/* pages/member/remainingSum/withdrawCash/cashList.wxss */
page{
  background-color: #f5f5f5;
}
.cashlist-item{
  width: 100%;
  background-color: #fff;
  margin-bottom: 20rpx;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  font-size: 26rpx;
}
.cashlist-top{
  overflow: hidden;
  margin-bottom: 20rpx;
}
.cashlist-bottom{
  overflow: hidden;
}
.cashlist-state{
  font-size: 26rpx;
  color: #ff3b44;
  color: #666;
}
.cashlist-num{
   color: #666; 
  font-size: 26rpx;
}
.cashlist-time{
   color: #999; 
  font-size: 26rpx;
  float: right;
}
.cashlist-sum{
  color: #ff3b44;
  font-size: 26rpx;
  float: right;
}
.loading{
  text-align:center;
  font-size:28rpx;
  margin:20rpx 0;
  color:#999;
}
.none-img{
  margin: 100rpx auto;
  text-align: center;
}
.none-img image{
  width: 200rpx;
  height: 200rpx;
}
.none-img text{
  display: block;
  color:#bbb;
  font-size:32rpx;
}
.cashlist {
    margin-top: 18rpx;
}